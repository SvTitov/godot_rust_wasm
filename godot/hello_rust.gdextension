[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../rust/target/debug/libhello_rust.so"
linux.release.x86_64 =   "res://../rust/target/release/libhello_rust.so"
windows.debug.x86_64 =   "res://../rust/target/debug/hello_rust.dll"
windows.release.x86_64 = "res://../rust/target/release/hello_rust.dll"
macos.debug =            "res://../rust/target/debug/libhello_rust.dylib"
macos.release =          "res://../rust/target/release/libhello_rust.dylib"
macos.debug.arm64 =      "res://../rust/target/debug/libhello_rust.dylib"
macos.release.arm64 =    "res://../rust/target/release/libhello_rust.dylib"
web.debug.wasm32 = 		 "res://../rust/target/wasm32-unknown-emscripten/debug/hello_rust.wasm"
web.release.wasm32 = 	 "res://../rust/target/wasm32-unknown-emscripten/release/hello_rust.wasm"